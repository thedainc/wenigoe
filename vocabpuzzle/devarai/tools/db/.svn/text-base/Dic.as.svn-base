package devarai.tools.db {
	import devarai.common.ui.IContentElement;
	
	public class Dic implements IContentElement {
		
		private var _on:int;
		private var _info:String;
		private var _descr:String;
		
		public function Dic(no:int, info:String, descr:String) {
			this._on = no;
			this._info = info;
			this._descr = descr;
		}
		
		public function get desc():String {
			return this._descr;
		}
		public function get no():int {
			return this._on;
		}
		public function get info():String {
			return this._info;
		}
		
		
	}
	
}